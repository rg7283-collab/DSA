#include <stdio.h>

/*
  Fibonacci Series Using Recursion
  Time Complexity: O(2^n)
  Space Complexity: O(n)
*/

int fibonacci(int n) {
    if (n == 0)
        return 0;
    if (n == 1)
        return 1;

    return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int n = 10;

    printf("Fibonacci Series: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", fibonacci(i));
    }

    return 0;
}
